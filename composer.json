{
  "name": "kent013/artisan-command-apib-resource",
  "description": "An artisan command to generate API resource from Factory/Enum.",
  "version": "1.0.0",
  "keywords": [
    "artisan",
    "laravel",
    "apib",
    "testcase"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Kentaro ISHITOYA",
      "email": "kentaro.ishitoya@gmail.com",
      "homepage": "https://github.com/kent013/"
    }
  ],
  "require": {
    "php": "^8.3",
    "laravel/framework": "^10.48",
    "bensampo/laravel-enum": "^6.11"
  },
  "autoload": {
    "psr-4": {
      "ArtisanCommandApibResource\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "tests/Fixtures/App/",
      "Database\\": "tests/Fixtures/Database/",
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ArtisanCommandApibResource\\ApibResourceGenerateCommandServiceProvider"
      ]
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.17",
    "orchestra/testbench": "^8.22",
    "phpstan/extension-installer": "^1.3.1",
    "phpstan/phpstan-webmozart-assert": "^1.2",
    "phpunit/phpunit": "^10.5",
    "larastan/larastan": "^2.9"
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
